{
  "compilerOptions": {
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "strict": true,
      "noEmit": true,
      "target": "esnext",
      "jsx": "react",
      "allowJs": true,
      "lib": ["dom", "es2015", "es2017"],
      "noImplicitReturns": true,
      "esModuleInterop": true,
  },
  "exclude": ["./node_modules", "./public", "./.cache"],
  "include": [
      "./src/**/*"
  ]
}